<%- include('./layout/header'); -%>
<%- include('./layout/partials/navbar'); -%>
<section class="section-theme">
    <div class="container">
        <div class="section-title">Dashboard</div>
        <p class="color-theme">Welcome <strong>Name</strong> </p>
        <div class="row mb-md-4 mb-3">
            <div class="col-lg-4 col-md-6 col-sm-8">
                <div class="html5-dialog">
                    <dialog id="window">
                        <form class="" action="/dashboard/admin" method="POST">
                            <h2 class="form-signin-heading">Add Admin</h2>
                            <div class="form-group">
                                <input type="text" id="name" name="adminName" class="form-control" placeholder="Enter a new admin name" required autofocus>
                            </div>
                            <div class="form-group">
                                <input type="text"   id="description" 
                                       name="description" class="form-control" placeholder="description"  >
                            </div>
                            <button id="exit" class="btn btn-outline-dark" >Exit</button>
                            <button class="btn btn-lg btn-default btn-block " type="submit" >Add</button>
                        </form>
                    </dialog>
                    <button id="show" class="btn btn-theme btn-success btn-block mb-2"><i class="fa fa-plus pr-3"></i> Add New Admin</button>
                </div>
                <div class="html5-dialog">
                    <dialog id="window">
                        <form class="" action="/dashboard/registerAddress" method="POST">
                            <h2 class="form-signin-heading">RegisTer New Addrees</h2>
                            <div class="form-group">
                                <input type="text"   id="address" 
                                       name="address" class="form-control" placeholder="Register an Address" required autofocus >
                            </div>
                            <button id="exit" class="btn btn-outline-dark" >Exit</button>
                            <button class="btn btn-lg btn-default btn-block" type="submit" >Add</button>
                        </form>
                    </dialog>
                    <button id="show" class="btn btn-theme btn-success btn-block"><i class="fa fa-plus pr-3"></i> Add Registered List</button>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <div class="table-responsive-sm">
                    <table id="dtBasicExample1" class="table-theme table table-sm" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th class="th-sm">User Name
                                </th>
                                <th class="th-sm">User Address
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for (var i = 0; i < 10; i++) { %>
                            <tr>
                                <td>Name <%- i -%></td>
                                <td>Address <%- i -%></td>
                            </tr>
                            <% } %>
                        </tbody>
                    </table>
                </div>
                <div class="table-responsive-sm">
                    <% if(list.length){ %>   
                    <table id="dtBasicExample" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th class="th-sm">Name
                                </th>
                                <th class="th-sm">Address
                                </th>
                            </tr>
                            <% } %>
                        </thead>
                        <tbody>
                            <% for (var i = 0; i < list.length; i++) { %>
                            <tr>
                                <td>
                                    <%= list[i].walletName %>
                                </td>
                                <td>
                                    <%= list[i].registered_address %>
                                </td>
                            </tr>
                            <% } %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="section-theme">
    <div class="container">
        <div class="section-title">RDD page</div>
        <nav class="nav-tabs-theme">
            <div class="nav nav-tabs" id="nav-tab" role="tablist">
                <a class="nav-item nav-link btn-theme btn-success active" id="nav-public-tab" data-toggle="tab" href="#nav-public" role="tab" aria-controls="nav-public" aria-selected="true">Public</a>
                <a class="nav-item nav-link btn-theme btn-success" id="nav-private-tab" data-toggle="tab" href="#nav-private" role="tab" aria-controls="nav-private" aria-selected="false">Private</a>
            </div>
        </nav>
        <div class="tab-content tab-content-theme box-theme p-3" id="nav-tabContent">
            <div class="tab-pane fade show active" id="nav-public" role="tabpanel" aria-labelledby="nav-public-tab">
                <p class="color-theme">Public</p>
                <div class="row">
                    <div class="col">
                        <div class="table-responsive-sm">
                            <table id="dtBasicExample1" class="table-theme table table-sm" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th class="th-sm">User Name
                                        </th>
                                        <th class="th-sm">User Address
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for (var i = 0; i < 10; i++) { %>
                                    <tr>
                                        <td>Name <%- i -%></td>
                                        <td>Address <%- i -%></td>
                                    </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div class="tab-pane fade" id="nav-private" role="tabpanel" aria-labelledby="nav-private-tab">
                <p class="color-theme">Private</p>
                <div class="row">
                    <div class="col">
                        <div class="table-responsive-sm">
                            <table id="dtBasicExample1" class="table-theme table table-sm" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th class="th-sm">User Name
                                        </th>
                                        <th class="th-sm">User Address
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for (var i = 0; i < 10; i++) { %>
                                    <tr>
                                        <td>Name <%- i -%></td>
                                        <td>Address <%- i -%></td>
                                    </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- JavaScript to provide the "Show/Close" functionality -->
<script type="text/JavaScript">
    (function() {  
    var dialog1 = document.getElementById('window');  
    var addButton = document.getElementById('show');  
    var exitButton = document.getElementById('exit');  
    addButton.onclick = function() {  
    dialog1.show();  
    //  addButton.dis
    };  
    exitButton.onclick = function() {  
    dialog1.close();  
    //addButton.show();
    };  
    })(); 
</script>  
